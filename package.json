{
  "name": "api-cinema",
  "description": "API feita para os alunos treinarem o desenvolvimento de apps consumindo um webservice",
  "version": "1.0.0",
  "main": "sources/app.js",
  "scripts": {
    "dev": "nodemon sources/app.js",
    "apidoc": "apidoc -f sources -e node_modules",
    "test": "./node_modules/.bin/mocha --reporter spec sources/app.spec.js"
  },
  "keywords": [],
  "author": "Fernando Brum <fernandobalcantara@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feealc/api-cinema.git"
  },
  "bugs": {
    "url": "https://github.com/feealc/api-cinema/issues"
  },
  "homepage": "https://github.com/feealc/api-cinema#readme",
  "dependencies": {
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.1",
    "express": "^4.14.1",
    "express-validator": "^3.1.2",
    "is-valid-date": "0.0.1",
    "jsonwebtoken": "^7.3.0",
    "md5": "^2.2.1",
    "mongoose": "^4.8.3",
    "nodemon": "^1.11.0",
    "valid-objectid": "0.0.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "mocha": "^3.2.0"
  },
  "apidoc": {
    "title": "API Cinema",
    "url": "https://api-cinema2.herokuapp.com/api",
    "order": [
      "Usuario",
      "getAllUsuarios",
      "Filme",
      "getAllFilmes",
      "getOneFilme",
      "createFilme",
      "updFullFilme",
      "updParcialFilme",
      "delFilme",
      "delAllFilmes"
    ],
    "template": {
      "forceLanguage": "pt_br",
      "withCompare": true,
      "withGenerator": true
    }
  }
}
